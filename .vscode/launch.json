    "version": "0.2.0",
    "configurations": [
        {
            "name": "user_debug",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "cwd": "${workspaceFolder}/user/",
            "envFile": ".env",
            "args": [
                "run",
                "--no-debugger",
                "--reload",
                "--port=2000"
